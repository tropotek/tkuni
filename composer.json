{
  "name": "ttek/tkuni",
  "type": "project",
  "description": "A base TK lib project for education institutions.",
  "keywords": [
    "tk-template",
    "tropotek"
  ],
  "time": "2019-04-09",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Michael Mifsud",
      "email": "info@tropotek.com.au",
      "homepage": "http://www.tropotek.com/"
    }
  ],
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Tk\\Composer\\SetupEvent::postInstall"
    ],
    "post-update-cmd": [
      "Tk\\Composer\\SetupEvent::postUpdate"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.2.x-dev"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/IMSGlobal/lti-1-3-php-library"
    }
  ],
  "require": {
    "php": "^7.3||^8.0",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",

    "ttek/tk-uni": "~3.6.0",
    "ttek/theme-cube": "~1.4.0",
    "ttek-plg/plg-ldap": "~3.4.0",
    "ttek-plg/mailog": "~3.0.0"
  },
  "require-dev": {
  },
  "config": {
    "allow-plugins": {
      "ttek/tk-installers": true
    }
  }
}
